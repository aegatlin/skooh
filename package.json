{
  "name": "skooh",
  "version": "0.2.0",
  "description": "skooh is a git hooks manager",
  "bin": "./skooh.js",
  "scripts": {
    "ppp": "npm version patch && npm publish && git push",
    "test": "npx skooh prepare",
    "prepare": "skooh"
  },
  "hooks": {
    "pre-commit": "echo 'pre-commit from main'",
    "post-checkout": "echo 'post-checkout from main'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aegatlin/skooh.git"
  },
  "keywords": [
    "git",
    "hooks",
    "manager",
    "hook",
    "package.json",
    "package",
    "simple",
    "minimal",
    "easy"
  ],
  "author": "Austin Gatlin",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/aegatlin/skooh/issues"
  },
  "homepage": "https://github.com/aegatlin/skooh",
  "devDependencies": {
    "skooh": "^0.1.4"
  }
}
